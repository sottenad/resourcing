<div class="container">
  <div class="row">
    <h3>Users</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @users.each do |user| %>
            <tr>
              <%= render user %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <div class="">

	    <%= simple_form_for(@new_user, html: {class:'form-horizontal'}) do |f| %>
	    	<%= f.input :email %>
	    	<%= f.button :submit %>
	    <% end %>
    </div>
    
  </div>
</div>
